# Agrega reglas comunes al .gitignore
echo "
# Archivos de Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Dependencias de Node (si usas JHipster/React/etc)
node_modules/
dist/
build/

# Archivos de Java compilados
*.class
*.jar
*.war
target/

# Archivos de IDE (VSCode, IntelliJ)
.vscode/
.idea/
*.iml

# Archivos de OS
.DS_Store
Thumbs.db
" >> .gitignore